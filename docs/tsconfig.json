{
  "extends": "../tsconfigs/tsconfig.browser.json",
  "compilerOptions": {
    "jsx": "preserve",
    "types": [
      "vite/client",
      "vitepress/client"
    ]
  },
  "include": [
    "./**/*.ts",
    "./**/*.ts",
    "./**/*.d.ts",
    "./**/*.tsx",
    "./**/*.vue",
    "./**/*.md",
    "./.i18n/index.ts",
    "./.vitepress/**/*.ts",
    "./.vitepress/**/*.tsx",
    "./.vitepress/**/*.vue"
  ],
  "exclude": [
    "./.vitepress/config.ts",
    "./**/*.node.ts"
  ],
  "vueCompilerOptions": {
    "vitePressExtensions": [".md"]
  }
}
